{
	"go.toolsEnvVars": {

	},

	"go.lintTool":"golangci-lint",
	"go.lintFlags": [
		"--fast",
		"-E", "goimports",
		"-E", "gocritic",
		"-E", "gocyclo",
		"-E", "gosec",
		"-E", "maligned",
		"-E", "scopelint",
		"-E", "interfacer",
		"-E", "goconst",
		"-E", "unconvert",
		"-E", "unparam",
		"-E", "prealloc",
		"-E", "varcheck",
],
"go.formatTool": "goimports",

}
